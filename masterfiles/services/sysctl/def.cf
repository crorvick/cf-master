bundle common sysctl_def_file_control
{
  vars:
      "input[sysctl_conf_selective_present]"
        string => "$(this.promise_dirname)/policy/sysctl_conf_selective_present.cf";

    any::
      "inputs" slist => getvalues(input);
}

body file control
{
  inputs => { @(sysctl_def_file_control.inputs) };
}